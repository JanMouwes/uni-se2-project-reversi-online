@using Reversi.Game
@{
    ViewData["Title"] = "Home Page";
}
<script>
    window.addEventListener("load",
        () => {
            var target = document.getElementById("main-board");
            
            ReversiModule.init(target);
        });
</script>
<style>
    #main-board {
        width: 600px;
        height: 600px;
    }
</style>
<div class="game-screen">
    <div id="scenario-menu" class="menu-left">
        <form id="scenario-form">
            <br/>
            <input type="submit"/>
        </form>
    </div>

    <div id="main-board" class="menu-right">

    </div>
</div>